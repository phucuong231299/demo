    .position{
      width: 100px;
      height: 100px;
      position: absolute;
      display: block;
      top: 1195px;
      left: 753px;
    }
    .marker{
      width: 10px;
      height: 10px;
      position: relative;
      top: 45px;
      left: 45px;
      background: rgba(255, 0, 0, 0.7);
      border-radius: 50%;
      z-index: 1000;
    }
    .marker-animation{
      width: 100%;
      height: 100%;
      position: absolute;
      display: block;
      top: 0;
      background: rgba(255, 0, 0, 0.7);
      border-radius: 50%;
      animation: pulse 2400ms ease-out infinite;
    }
    .marker-animation1{
      width: 100%;
      height: 100%;
      position: absolute;
      display: block;
      top: 0;
      background: rgba(255, 0, 0, 0.7);
      border-radius: 50%;
      opacity: 0;
      animation: pulse 2400ms ease-out infinite;
      animation-delay: .5s;
    }
    @keyframes pulse{
      0%{
        transform: scale(0.1);
        opacity: 0;
      }
      50%{
        opacity: 0.8;
      }
      100%{
        transform: scale(1.2);
        opacity: 0;
      }
    }
  </style>